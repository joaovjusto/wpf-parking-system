<Window x:Class="SistemaEstacionamento.Views.CadastroUsuario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SistemaEstacionamento.Views"
        mc:Ignorable="d"
        Title="CadastroUsuario" Height="271.5" Width="330.5" Background="#FF5D5D5D">

    <Grid HorizontalAlignment="Left" Height="269" Margin="0,-26,0,-2" VerticalAlignment="Top" Width="320">
        <TextBox HorizontalAlignment="Center" Height="26" Margin="68,67,84,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="168" x:Name="usuario" Text="Insira o nome desejado" TextChanged="Usuario_TextChanged"/>
        <TextBox HorizontalAlignment="Center" Height="26" Margin="68,118,84,0" TextWrapping="Wrap" Text="Insira a senha desejada" VerticalAlignment="Top" Width="168" x:Name="senha"/>
        <Button Content="Cadastrar" HorizontalAlignment="Left" Margin="116,168,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>